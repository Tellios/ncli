{
  "name": "tellios-ncli",
  "version": "0.0.1",
  "description": "Helper scripts for easing workflow",
  "main": "index.js",
  "engines": {
    "node": ">= 6.10.3"
  },
  "bin": {
    "na": "./bin/alias",
    "nclone": "./bin/git/clone",
    "ncommit": "./bin/git/commit",
    "ncb": "./bin/git/createBranch",
    "ngcb": "./bin/git/getCurrentBranch",
    "ngb": "./bin/git/getBranches",
    "nmf": "./bin/git/mergeFrom",
    "nmt": "./bin/git/mergeTo",
    "nrb": "./bin/git/removeBranch",
    "nstatus": "./bin/git/status",
    "nsb": "./bin/git/switchBranch",
    "npull": "./bin/git/pull",
    "npush": "./bin/git/push",
    "nupstream": "./bin/git/setUpstream",
    "ndps": "./bin/docker/processes",
    "ndrm": "./bin/docker/remove",
    "ndstart": "./bin/docker/start",
    "ndstop": "./bin/docker/stop"
  },
  "scripts": {
    "pretty": "prettier --config .prettierrc.json --write \"{lib,src}/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watchAll --runInBand",
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "build:watch": "rimraf dist && tsc --watch -p tsconfig.build.json"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "chalk": "^2.4.1",
    "cli-table2": "^0.2.0",
    "fs-extra": "^5.0.0",
    "js-yaml": "^3.11.0",
    "lodash": "^4.17.10",
    "nodegit": "^0.20.3",
    "readline-sync": "^1.4.9",
    "yargs": "^8.0.1"
  },
  "devDependencies": {
    "@types/cli-table2": "^0.2.1",
    "@types/fs-extra": "^5.0.2",
    "@types/jest": "^22.2.3",
    "@types/js-yaml": "^3.11.1",
    "@types/lodash": "^4.14.108",
    "@types/node": "^9.6.7",
    "@types/nodegit": "^0.18.7",
    "@types/readline-sync": "^1.4.3",
    "@types/sinon": "^4.3.1",
    "@types/yargs": "^11.0.0",
    "jest": "^20.0.4",
    "prettier": "^1.12.1",
    "rimraf": "^2.6.2",
    "sinon": "^2.3.2",
    "ts-jest": "^22.4.4",
    "typescript": "^2.8.3"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
